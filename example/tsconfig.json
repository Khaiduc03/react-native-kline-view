{
  "extends": "@react-native/typescript-config",
  "compilerOptions": {
    "noEmit": true,
    "strict": true,
    "jsx": "react-native",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "types": ["jest"],
    "baseUrl": ".",
    "paths": {
      "react-native-kline-view": ["../"]
    }
  },
  "include": ["./**/*"],
  "exclude": ["node_modules", "android", "ios"]
}
